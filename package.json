{
  "name": "cms-saf",
  "version": "1.0.0",
  "private": true,
  "babel": {
    "presets": [
      [
        "@babel/preset-env"
      ],
      [
        "@vue/app",
        {
          "useBuiltIns": "entry"
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-nullish-coalescing-operator"
    ]
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "release-it": "npm version patch && git push --follow-tags",
    "cy:run": "cypress run"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@highlightjs/vue-plugin": "^1.0.2",
    "@mdi/font": "^5.9.55",
    "@types/prismjs": "^1.16.6",
    "babel-plugin-prismjs": "2.0.1",
    "babel-polyfill": "^6.26.0",
    "cache-loader": "^4.1.0",
    "core-js": "^3.21.1",
    "css-vars-ponyfill": "^2.4.6",
    "highlight.js": "^11.3.1",
    "intersection-observer": "^0.12.0",
    "material-design-icons": "^3.0.1",
    "node-gyp": "^8.4.0",
    "prismjs": "^1.25.0",
    "register-service-worker": "^1.7.2",
    "vue": "^2.6.12",
    "vue-cheetah-grid": "^1.1.5",
    "vue-json-viewer": "^2.2.20",
    "vue-prism-component": "1.2.0",
    "vue-router": "^3.5.2",
    "vuetify": "^2.6.10",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.14.5",
    "@babel/preset-env": "^7.14.9",
    "@vue/cli": "^4.5.16",
    "@vue/cli-plugin-babel": "^4.5.16",
    "@vue/cli-plugin-eslint": "^4.5.16",
    "@vue/cli-plugin-pwa": "^4.5.16",
    "@vue/cli-service": "^4.5.16",
    "@vue/cli-service-global": "^4.5.16",
    "babel-eslint": "^10.1.0",
    "cypress": "^7.6.0",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^7.15.1",
    "sass": "^1.37.2",
    "sass-loader": "^7.3.1",
    "vue-cli-plugin-vuetify": "^2.4.1",
    "vue-template-compiler": "^2.6.12",
    "vuetify-loader": "^1.7.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "vue/valid-v-slot": "off",
      "vue/no-unused-vars": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
